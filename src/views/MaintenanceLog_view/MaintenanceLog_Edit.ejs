<%- include('partials/_header'); -%>
    <div class="container mt-5">
        <div class="row">
            <div class="col-md-5 offset-md-3 card">
                <div class="card-body">
                    <form action="/mantening/update/<%= data.maintenance_id %>" method="post">
                        <div class="form-group">
                            <input type="number" name="business_name" value="<%= data.vehicle_id%>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="phone_number" value="<%= data.maintenance_type%>" class="form-control" autofocus>
                        </div>
                        <div class="form-group">
                            <input type="date" name="email" value="<%= data.maintenenace_day%>" class=form-control">
                        </div>
                        <button type="submit" class="btn btn-primary">
                            Edit maintenance
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
<%- include('partials/_footer'); -%>