<%- include('partials/_header'); -%>
    <div class="col-md-5">
        <div class="card-body">
            <form action="/mantening/add" method="POST">
                <div class="form-group">
                    <input type="number" name="vehicle_id" placeholder="Vehicle_id" class="form-control" required>
                </div>
                <div class="form-group">
                    <input type="text" name="maintenance_type" placeholder="maintenance_type" class="form-control" required>
                </div>
                <div class="form-group">
                    <input type="date" name="maintenenace_day" placeholder="maintenenace_day" class="form-control" required>
                </div>
                <button type="submit" class="btn btn-primary">
                    Add mantening
                </button>

            </form>
        </div>
    </div>

<%- include('partials/_footer'); -%>